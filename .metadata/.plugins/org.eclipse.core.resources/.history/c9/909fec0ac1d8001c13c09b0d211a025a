package com.lms.accesslibrary.dao;

import com.lms.accesslibrary.entity.library.book.Book;
import com.lms.accesslibrary.entity.library.user.User;

public abstract class BaseDAO implements iDAO{	
	public abstract BookBean insertBook(Book book);
	public abstract BookBean updateBook(Book book);		
	public abstract BookBean getBookById(int id);		
	public abstract void deleteBookById(int id);
	
	public abstract UserBean insertUser(User user);
	public abstract UserBean updateUser(User user);
	public abstract UserBean getUserById(int id);
	public abstract void deleteUserById(int id);
}
