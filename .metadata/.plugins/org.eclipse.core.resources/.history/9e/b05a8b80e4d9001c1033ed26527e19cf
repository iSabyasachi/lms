package com.lms.accesslibrary.dao;

import java.sql.ResultSet;
import java.sql.SQLException;
import java.util.Date;
import java.util.Set;

import org.springframework.jdbc.core.RowMapper;

import com.lms.accesslibrary.entity.library.book.Book;
import com.lms.accesslibrary.entity.library.book.BookItem;

public class BookMapper implements RowMapper<Book>{

	@Override
	public Book mapRow(ResultSet rs, int rowNum) throws SQLException {
		Book book = new Book();		
		
		book.setTitle(rs.getString("title"));
		book.setType(rs.getString("title"));
		book.setSubject(rs.getString("title"));
		book.setPublisher(rs.getString("title"));
		book.setPublicationDate(rs.getString("title"));
		book.setLanguage(rs.getString("title"));
		book.setNumberOfPages(rs.getString("title"));
		book.setAuthor(rs.getString("title"));
		book.setDescription(rs.getString("title"));
		book.setGenre(rs.getString("title"));
		book.setIsReferenceOnly(rs.getString("title"));
		book.setActive(rs.getString("title"));
		book.setUnit(rs.getString("title"));
		book.setRack_num(rs.getString("title"));
		book.setStatus(rs.getString("title"));
		
		return null;
	}

}
